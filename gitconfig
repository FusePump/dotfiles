[core]
    excludesfile = ~/.cvsignore
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    tree = log --graph --pretty=oneline --abbrev-commit --all
[color]
    ui = true
[http]
    sslVerify = false
[credential]
    helper = cache --timeout=28800
